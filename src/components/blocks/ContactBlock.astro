---
import type { ContactBlock } from "../../content/config";
import Markdown from "../Markdown.astro";
import ContactBlockForm from "./ContactBlockForm.astro";
import UIBlock from "./UIBlock.astro";

const block = Astro.props as ContactBlock;
const { content } = block;
---

<UIBlock {...block}>
  <Markdown md={content} />
  <ContactBlockForm />
</UIBlock>
